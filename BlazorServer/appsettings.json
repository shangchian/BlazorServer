{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}


/*
  "Default": "Information" : 代表印 log 等級比 Information 高的訊息才會被印出來

    Trace: 1
    Debug: 2
    Information: 3
    Warning: 4
    Error: 5
    Critical: 6
*/
